class Solution:
    def maximumPopulation(self, logs):
        year_changes = [0] * 101  

        for birth, death in logs:
            year_changes[birth - 1950] += 1
            year_changes[death - 1950] -= 1

        max_pop = 0
        curr_pop = 0
        earliest_year = 1950

        for i in range(101):  
            curr_pop += year_changes[i]
            if curr_pop > max_pop:
                max_pop = curr_pop
                earliest_year = 1950 + i

        return earliest_year
